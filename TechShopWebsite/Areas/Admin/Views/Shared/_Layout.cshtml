<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title</title>
    <!-- BOOTSTRAP STYLES-->
    <link href="~/Content/assetsAdmin/css/bootstrap.css" rel="stylesheet" />
    <!-- FONTAWESOME STYLES css/font-awesome.css-->
    <link href="~/Content/assetsAdmin/css/font-awesome.css" rel="stylesheet" />
    <!-- MORRIS CHART STYLES /js/morris/morris-0.4.3.min.css-->
    <link href="~/Content/assetsAdmin/js/morris/morris-0.4.3.min.css" rel="stylesheet" />
    <!-- CUSTOM STYLES-->
    <link href="~/Content/assetsAdmin/css/custom.css" rel="stylesheet" />
    <!-- GOOGLE FONTS-->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css' />
    <link href="~/Content/assetsAdmin/js/dataTables/dataTables.bootstrap.css" rel="stylesheet" />
    <link href="/ckfinder/userfiles/files/public/Logo.png" rel="icon">


</head>
<body>
    <div id="wrapper">
        @Html.Partial("_PartialSideBar")
        <!-- /. NAV SIDE  -->
        <div id="page-wrapper">
            @RenderBody()
            <!-- /. PAGE INNER  -->
        </div>
        <!-- /. PAGE WRAPPER  -->
    </div>
    <!-- /. WRAPPER  -->
    <!-- SCRIPTS -AT THE BOTOM TO REDUCE THE LOAD TIME js/jquery-1.10.2.js-->
    <!-- JQUERY SCRIPTS -->
    <script src="~/Content/assetsAdmin/js/jquery-1.10.2.js"></script>
    <!-- BOOTSTRAP SCRIPTS -->
    <script src="~/Content/assetsAdmin/js/bootstrap.min.js"></script>
    <!-- METISMENU SCRIPTS -->
    <script src="~/Content/assetsAdmin/js/jquery.metisMenu.js"></script>
    <!-- MORRIS CHART SCRIPTS -->
    <script src="~/Content/assetsAdmin/js/morris/raphael-2.1.0.min.js"></script>
    <script src="~/Content/assetsAdmin/js/morris/morris.js"></script>

    <!-- CUSTOM SCRIPTS -->
    <script src="~/Content/assetsAdmin/js/custom.js"></script>
    <script src="~/Content/ckeditor/ckeditor/ckeditor.js"></script>
    <script src="~/Content/ckfinder/ckfinder/ckfinder.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    <script src="https://cdn.ckeditor.com/ckeditor5/36.0.1/classic/ckeditor.js"></script>

    <hr />
    <footer>
        <p>&copy; @DateTime.Now.Year </p>
    </footer>
</body>
</html>